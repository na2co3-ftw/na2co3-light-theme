{
    "name": "na2co3 Light",
    "tokenColors": [
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "foreground": "#229900"
            }
        },
        {
            "name": "Structural Keyword",
            "scope": ["keyword.control", "storage"],
            "settings": {
                "fontStyle": "",
                "foreground": "#8800ff"
            }
        },
        {
            "name": "Keyword",
            "scope": ["keyword", "support", "variable.language"],
            "settings": {
                "foreground": "#006666"
            }
        },
        {
            "name": "Number",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#0000cc"
            }
        },
        {
            "name": "Constant",
            "scope": "constant.language",
            "settings": {
                "foreground": "#0066cc"
            }
        },
        {
            "name": "String",
            "scope": "string",
            "settings": {
                "fontStyle": "",
                "foreground": "#cc0000"
            }
        },
        {
            "name": "Operator",
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#886600"
            }
        },
        {
            "name": "Annotation",
            "scope": [
                "storage.type.annotation",
                "punctuation.definition.annotation",
                "punctuation.definition.decorator",
                "punctuation.decorator",
                "entity.name.function.decorator",
                "support.other.attribute",
                "storage.modifier.attribute",
            ],
            "settings": {
                "foreground": "#808000",
            }
        },
        {
            "name": "Escape Sequence",
            "scope": [
                "constant.character",
                "string punctuation.definition.template-expression",
                "string punctuation.definition.interpolation",
                "punctuation.section.embedded"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Invalid",
            "scope": "invalid",
            "settings": {
                "foreground": "#ff0000"
            }
        },
        {
            "name": "Variable in String",
            "scope": [
                "string.quoted.double.php variable.other.php",
                "string.unquoted.heredoc.php variable.other.php",
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#0066ff"
            }
        },
        {
            "name": "String.regexp",
            "scope": [
                "string.regexp",
                "constant.regexp"
            ],
            "settings": {
                "foreground": "#cc5500"
            }
        },
        {
            "name": "Language Variable (exclude LLVM)",
            "scope": "variable.language.llvm",
            "settings": {
                "foreground": "#222222"
            }
        },
        {
            "name": "Embedded Source",
            "scope": [
                "meta.embedded",
                "meta.interpolation",
                "source.groovy.embedded",
                "string meta.image.inline.markdown"
            ],
            "settings": {
                "foreground": "#222222"
            }
        },
        {
            "name": "Tag",
            "scope": [
                "text.html meta.tag",
                "text.xml meta.tag",
                "entity.name.tag",
                "punctuation.definition.tag",
            ],
            "settings": {
                "foreground": "#bb0099"
            }
        },
        {
            "name": "Tag Attribute",
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#aa6600"
            }
        },
        {
            "name": "CSS id",
            "scope": [
                "entity.other.attribute-name.id.css",
                "source.css.less entity.other.attribute-name.id"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#aa6600"
            }
        },
        {
            "name": "CSS Pseudo Class/Element",
            "scope": [
                "entity.other.attribute-name.pseudo-class.css",
                "entity.other.attribute-name.pseudo-element.css",
            ],
            "settings": {
                "foreground": "#ee5500",
            }
        },
        {
            "name": "CSS Property",
            "scope": [
                "support.type.property-name.css",
                "support.type.property-name.media.css",
                "support.type.vendored.property-name.css",
            ],
            "settings": {
                "foreground": "#990000"
            }
        },
        {
            "name": "CSS Color Code",
            "scope": [
                "constant.other.color.rgb-value",
                "constant.other.rgb-value",
            ],
            "settings": {
                "foreground": "#0000cc"
            }
        },
        {
            "name": "CSS Important",
            "scope": [
                "keyword.other.important.css",
                "keyword.other.important.scss"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#006666"
            }
        },
        {
            "name": "PHP Tag",
            "scope": [
                "punctuation.section.embedded.begin.php",
                "punctuation.section.embedded.end.php"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#bb0099"
            }
        },
        {
            "name": "Language Specific Structural Keyword",
            "scope": [
                "source.cs keyword.other",
                "keyword.declaration.dart",
                "keyword.local.lua",
                "keyword.other.rust",
                "keyword.other.fn.rust"
            ],
            "settings": {
                "foreground": "#8800ff"
            }
        },
        {
            "name": "Language Specific Keyword",
            "scope": [
                "source.cs keyword.other",
                "keyword.declaration.dart",
                "keyword.local.lua",
                "keyword.other.rust",
                "keyword.other.fn.rust",
                "source.sql keyword.other",
                "source.swift keyword.other"
            ],
            "settings": {
                "foreground": "#8800ff"
            }
        },
        {
            "name": "Java import/package",
            "scope": [
                "storage.modifier.import.java",
                "storage.modifier.package.java",
            ],
            "settings": {
                "foreground": "#222222"
            }
        },
        {
            "name": "Java Type Name",
            "scope": [
                "source.java storage.type",
            ],
            "settings": {
                "foreground": "#006666"
            }
        },
        {
            "name": "Markup Bold",
            "scope": [
                "markup.bold",
                "markup.bold string.other.link.title.markdown"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#0000aa"
            }
        },
        {
            "name": "Markup Italic",
            "scope": [
                "markup.italic",
                "markup.italic string.other.link.title.markdown"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#0000aa"
            }
        },
        {
            "name": "Markup Strikethrough",
            "scope": [
                "markup.strikethrough",
                "markup.strikethrough string.other.link.title.markdown"
            ],
            "settings": {
                "fontStyle": "strikethrough"
            }
        },
        {
            "name": "Markup Heading",
            "scope": [
                "markup.heading",
                "markup.heading string.other.link.title.markdown"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#ff0088"
            }
        },
        {
            "name": "Markup List",
            "scope": "punctuation.definition.list.begin.markdown",
            "settings": {
                "foreground": "#0066cc"
            }
        },
        {
            "name": "Markup Quote",
            "scope": [
                "markup.quote",
                "markup.quote string.other.link.title.markdown"
            ],
            "settings": {
                "foreground": "#000088"
            }
        },
        {
            "name": "Markup Code",
            "scope": ["markup.inline.raw", "markup.fenced_code"],
            "settings": {
                "foreground": "#cc0000"
            }
        },
        {
            "name": "Markup Pre",
            "scope": "markup.raw.block",
            "settings": {
                "foreground": "#006000"
            }
        },
        {
            "name": "Markup Link",
            "scope": [
                "meta.link.inline.markdown",
                "meta.link.reference.markdown",
                "meta.link.reference.def.markdown",
                "markup.underline.link",
                "meta.image.inline.markdown",],
            "settings": {
                "foreground": "#0000ff"
            }
        },
        {
            "name": "Markup Link Title",
            "scope": "string.other.link.title.markdown",
            "settings": {
                "foreground": "#222222"
            }
        },
        {
            "name": "Markup Underline",
            "scope": "markup.underline",
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "name": "Markup separator",
            "scope": "meta.separator",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#000044"
            }
        },
        {
            "name": "Diff Header",
            "scope": [
                "meta.diff.header"
            ],
            "settings": {
                "foreground": "#8800ff"
            }
        },
        {
            "name": "Diff Range",
            "scope": [
                "meta.diff.range",
            ],
            "settings": {
                "foreground": "#886600"
            }
        },
        {
            "name": "Diff Deleted",
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#dd0000"
            }
        },
        {
            "name": "Diff Inserted",
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#009900"
            }
        },
        {
            "name": "Diff Changed",
            "scope": "markup.changed",
            "settings": {
                "foreground": "#0000cc"
            }
        },
    ],
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "namespace": "#006666",
        "class": "#006666",
        "enum": "#006666",
        "interface": "#006666",
        "struct": "#006666",
        "typeParameter": "#20999d",
        "type": "#006666",
        "parameter": {
            "foreground": "#6666ff",
            "fontStyle": "italic"
        },
        "variable": {
            "fontStyle": "italic"
        },
        "variable.local":{
            "foreground": "#6666ff",
            "fontStyle": ""
        },
        "property": "#222222",
        "property.static": {
            "fontStyle": "italic",
        },
        "enumMember": {
            "foreground": "#222222",
            "fontStyle": "italic",
        },
        "decorator": "#808000",
        "event": "#222222",
        "event.static": {
            "fontStyle": "italic",
        },
        "function": {
            "foreground": "#222222",
            "fontStyle": "italic"
        },
        "function.local":{
            "fontStyle": ""
        },
        "method": "#222222",
        "method.static": {
            "fontStyle": "italic",
        },
    },
    "colors": {
        "editor.background": "#f4f4f4",
        "editor.foreground": "#222222",
        "editorLineNumber.foreground": "#777777",
        "editorLineNumber.activeForeground": "#222222",
        "editor.selectionBackground": "#44aaff40",
        "editor.wordHighlightBackground": "#00000018",
        "editor.wordHighlightTextBackground": "#00000018",
        "editor.wordHighlightStrongBackground": "#ff00ff18",
        "editor.selectionHighlightBorder": "#44aaff80",
        "editor.selectionHighlightBackground": "#00000000",
        "editor.findMatchBackground": "#ffff00",
        "editor.findMatchHighlightBackground": "#ffaa0080",
        "editor.lineHighlightBackground": "#deebf5",
        "editor.rangeHighlightBackground": "#ffff0040",
        "editorWhitespace.foreground": "#00000030",
        "editorIndentGuide.background1": "#00000020",
        "editorIndentGuide.activeBackground1": "#00000040",
        "editorInlayHint.background": "#80808030",
        "editorInlayHint.foreground": "#606060",
        "editor.linkedEditingBackground": "#0080ff28",
        "editorBracketMatch.border": "#0080ffcc",
        "editorBracketMatch.background": "#00000000",
        "editorOverviewRuler.wordHighlightForeground": "#00000040",
        "editorOverviewRuler.wordHighlightTextForeground": "#00000040",
        "editorOverviewRuler.wordHighlightStrongForeground": "#ff00ff40",
        "editorOverviewRuler.findMatchForeground": "#ffaa00A0",
        "editorOverviewRuler.bracketMatchForeground": "#0080ff40",
        "editorOverviewRuler.modifiedForeground": "#bbccff",
        "editorOverviewRuler.addedForeground": "#aaddaa",
        "editorOverviewRuler.deletedForeground": "#ffccbb",
        "editorGutter.modifiedBackground": "#8090ff",
        "editorGutter.addedBackground": "#80cc80",
        "editorGutter.deletedBackground": "#ff9080",
        "diffEditor.insertedTextBackground": "#80cc80a0",
        "diffEditor.removedTextBackground": "#ff9080a0",
        "diffEditor.insertedLineBackground": "#80cc8050",
        "diffEditor.removedLineBackground": "#ff908050",
        "diffEditorGutter.insertedLineBackground": "#80cc8050",
        "diffEditorGutter.removedLineBackground": "#ff908050",
        "mergeEditor.conflict.input2.background": "#ff000080",
        "editorGroup.border": "#aaaaaa",
        "editorGroupHeader.tabsBackground": "#d0d0d0",
        "editorGroup.emptyBackground": "#d0d0d0",
        "tab.activeBackground": "#ffffff",
        "tab.activeForeground": "#222222",
        "tab.inactiveBackground": "#e4e4e4",
        "tab.inactiveForeground": "#222222",
        "tab.unfocusedActiveForeground": "#888888",
        "tab.unfocusedInactiveForeground": "#888888",
        "tab.border": "#aaaaaa",
        "tab.lastPinnedBorder": "#aaaaaa",
        "sideBar.border": "#aaaaaa",
        "sideBar.background": "#e4e4e4",
        "sideBar.foreground": "#222222",
        "panel.background": "#ffffff",
        "panel.border": "#aaaaaa",
        "list.inactiveSelectionBackground": "#20284020",
        "settings.textInputBorder": "#cecece",
        "settings.numberInputBorder": "#cecece",
        "gitDecoration.addedResourceForeground": "#208000",
        "gitDecoration.modifiedResourceForeground": "#0040ff",
        "gitDecoration.deletedResourceForeground": "#c00000",
        "gitDecoration.renamedResourceForeground": "#8000a0",
        "gitDecoration.stageModifiedResourceForeground": "#0040ff",
        "gitDecoration.stageDeletedResourceForeground": "#c00000",
        "gitDecoration.untrackedResourceForeground": "#208000",
        "gitDecoration.conflictingResourceForeground": "#ff2000",
    }
}