{
    "tokenColors": [
        {
            "name": "Comment",
            "scope": "comment",
            "settings": {
                "foreground": "#229900"
            }
        },
        {
            "name": "Number",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#0000CC"
            }
        },
        {
            "name": "Constant",
            "scope": "constant.language",
            "settings": {
                "foreground": "#0066CC"
            }
        },
        {
            "name": "Keyword",
            "scope": "keyword",
            "settings": {
                "foreground": "#006666"
            }
        },
        {
            "name": "Operator",
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#886600"
            }
        },
        {
            "name": "Keyword control",
            "scope": "keyword.control",
            "settings": {
                "fontStyle": "",
                "foreground": "#8800FF"
            }
        },
        {
            "name": "Storage",
            "scope": "storage",
            "settings": {
                "fontStyle": "",
                "foreground": "#8800FF"
            }
        },
        {
            "name": "Annotation",
            "scope": [
                "storage.type.annotation",
                "punctuation.definition.annotation",
                "punctuation.definition.decorator",
                "punctuation.decorator",
                "entity.name.function.decorator",
            ],
            "settings": {
                "foreground": "#808000",
            }
        },
        {
            "name": "Constant character",
            "scope": "constant.character",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markup Bold",
            "scope": ["markup.bold", "meta.separator.markdown"],
            "settings": {
                "fontStyle": "bold", "foreground": "#0000aa"
            }
        },
        {
            "name": "Markup Italic",
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#0000aa"
            }
        },
        {
            "name": "Markup Heading",
            "scope": "markup.heading",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#ff0088"
            }
        },
        {
            "name": "Markup List",
            "scope": "beginning.punctuation.definition.list.markdown",
            "settings": {
                "foreground": "#0088ff"
            }
        },
        {
            "name": "Markup Quote",
            "scope": "markup.quote",
            "settings": {
                "foreground": "#000088"
            }
        },
        {
            "name": "Markup Quote Punctuation",
            "scope": "beginning.punctuation.definition.quote.markdown",
            "settings": {
                "foreground": "#0000ff"
            }
        },
        {
            "name": "Markup Code",
            "scope": ["markup.fenced_code", "markup.inline.raw.string"],
            "settings": {
                "foreground": "#cc0000"
            }
        },
        {
            "name": "Markup Pre",
            "scope": "markup.raw.block",
            "settings": {
                "foreground": "#006000"
            }
        },
        {
            "name": "Markup String",
            "scope": "text.html.markdown string",
            "settings": {
                "foreground": "#222222"
            }
        },
        {
            "name": "Markup String in Bold",
            "scope": "text.html.markdown markup.bold string",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#0000aa"
            }
        },
        {
            "name": "Markup String in Italic",
            "scope": "text.html.markdown markup.italic string",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#0000aa"
            }
        },
        {
            "name": "Markup Link",
            "scope": ["meta.link.inline.markdown", "meta.image.inline.markdown", "meta.link.reference.markdown", "meta.link.reference.def.markdown"],
            "settings": {
                "foreground": "#0000ff"
            }
        },
        {
            "name": "String",
            "scope": "string",
            "settings": {
                "fontStyle": "",
                "foreground": "#CC0000"
            }
        },
        {
            "name": "Support",
            "scope": "support",
            "settings": {
                "foreground": "#006666"
            }
        },
        {
            "name": "Variable",
            "scope": "variable.language",
            "settings": {
                "foreground": "#006666"
            }
        },
        {
            "name": "Variable (exclude LLVM)",
            "scope": "variable.language.llvm",
            "settings": {
                "foreground": "#222222"
            }
        },
        {
            "name": "Invalid",
            "scope": "invalid",
            "settings": {
                "foreground": "#FF0000"
            }
        },
        {
            "name": "String variable",
            "scope": "string variable",
            "settings": {
                "fontStyle": "",
                "foreground": "#0066FF"
            }
        },
        {
            "name": "String embedded variable",
            "scope": "string source.php.embedded variable",
            "settings": {
                "foreground": "#222222"
            }
        },
        {
            "name": "String.regexp",
            "scope": [
                "string.regexp",
                "constant.regexp"
            ],
            "settings": {
                "foreground": "#CC5500"
            }
        },
        {
            "name": "Embedded Source",
            "scope": "text source",
            "settings": {
                "foreground": "#222222"
            }
        },
        {
            "name": "Tag",
            "scope": [
                "text.html meta.tag",
                "text.xml meta.tag",
                "name.tag.xhtml",
                "entity.name.tag",
                "punctuation.definition.tag"
            ],
            "settings": {
                "foreground": "#BB0099"
            }
        },
        {
            "name": "Tag Attribute",
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#AA6600"
            }
        },
        {
            "name": "CSS: #id",
            "scope": "entity.other.attribute-name.id.css",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#AA6600"
            }
        },
        {
            "name": "CSS: :pseudo",
            "scope": [
                "entity.other.pseudo-class",
                "entity.other.pseudo-element"
            ],
            "settings": {
                "foreground": "#AA6600"
            }
        },
        {
            "name": "CSS: property",
            "scope": [
                "support.type.property-name.css",
                "support.type.property-name.media.css",
                "support.constant.media.css"
            ],
            "settings": {
                "foreground": "#990000"
            }
        },
        {
            "name": "CSS: value keyword",
            "scope": [
                "support.constant.property-value.css",
                "support.constant.font-name.css",
                "support.constant.color",
                "support.function.url.css",
                "support.function.misc.css",
                "keyword.operator.logic.media.css",
                "meta.property-value.css keyword"
            ],
            "settings": {
                "foreground": "#006666"
            }
        },
        {
            "name": "CSS: value Number",
            "scope": [
                "constant.other.color.rgb-value.hex.css",
                "constant.numeric.css"
            ],
            "settings": {
                "foreground": "#0000CC"
            }
        },
        {
            "name": "CSS: !important",
            "scope": "keyword.other.important.css",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#006666"
            }
        },
        {
            "name": "diff.header",
            "scope": [
                "meta.diff",
                "meta.diff.header"
            ],
            "settings": {
                "foreground": "#229900"
            }
        },
        {
            "name": "diff.deleted",
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#FF2222"
            }
        },
        {
            "name": "diff.inserted",
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#2266AA"
            }
        },
        {
            "name": "diff.changed",
            "scope": "markup.changed",
            "settings": {
                "foreground": "#AA6622"
            }
        },
        {
            "name": "Meta.separator",
            "scope": "meta.separator",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#19356D"
            }
        }
    ],
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "namespace": "#222222",
        "type": "#222222",
        "struct": "#222222",
        "class": "#222222",
        "interface": "#222222",
        "enum": "#222222",
        "typeParameter": "#20999D",

        "function": {
            "foreground": "#222222",
            "fontStyle": "italic"
        },
        "function.local":{
            "fontStyle": ""
        },

        "variable": {
            "fontStyle": "italic"
        },
        "variable.local":{
            "foreground": "#6666FF",
            "fontStyle": ""
        },
        "parameter": {
            "foreground": "#6666FF",
            "fontStyle": "italic"
        },

        "property": "#222222",
        "member": "#222222",
    },
    "colors": {
        "editor.background": "#f4f4f4",
        "editor.foreground": "#222222",
        "editorCursor.foreground": "#000000",
        "editor.selectionBackground": "#44AAFF40",
        "editorLineNumber.foreground": "#777777",
        "editor.wordHighlightBackground": "#0000ff18",
        "editor.wordHighlightStrongBackground": "#ff00ff18",
        "editor.findMatchBackground": "#ffff00",
        "editor.findMatchHighlightBackground": "#ffbb44",
        "editor.lineHighlightBackground": "#44aaff20",
        "editorWhitespace.foreground": "#00000030",
        "editorIndentGuide.background": "#00000010",
        "editorBracketMatch.border": "#ff00ffcc",
        "editorBracketMatch.background": "#00000000",
        "editorOverviewRuler.findMatchForeground": "#ffbb44",
        "editorOverviewRuler.selectionHighlightForeground": "#00000020",
        "editorOverviewRuler.wordHighlightForeground": "#baa8ff",
        "editorOverviewRuler.wordHighlightStrongForeground": "#f0adf0",
        "editorOverviewRuler.modifiedForeground": "#b8cbf5",
        "editorOverviewRuler.addedForeground": "#aadeaa",
        "editorOverviewRuler.deletedForeground": "#ffc8bd",
        "editorInfo.foreground": "#cccccc",
        "editorGutter.modifiedBackground": "#b8cbf5",
        "editorGutter.addedBackground": "#aadeaa",
        "editorGutter.deletedBackground": "#ffc8bd",
        "diffEditor.insertedTextBackground": "#bee6be",
        "diffEditor.removedTextBackground": "#ffd5cc",
        "panel.border": "#888888",
        "panel.background": "#ffffff",
        "sideBar.border": "#888888",
        "sideBar.background": "#e4e4e4",
        "sideBar.foreground": "#222222",
        "gitDecoration.modifiedResourceForeground": "#0000ff",
        "editorGroup.emptyBackground": "#d0d0d0",
        "editorGroup.border": "#888888",
        "editorGroupHeader.tabsBackground": "#d0d0d0",
        "tab.activeBackground": "#ffffff",
        "tab.activeForeground": "#222222",
        "tab.inactiveBackground": "#e4e4e4",
        "tab.inactiveForeground": "#222222",
        "tab.unfocusedActiveForeground": "#888888",
        "tab.unfocusedInactiveForeground": "#888888",
        "tab.border": "#aaaaaa"
    },
    "name": "Light"
}